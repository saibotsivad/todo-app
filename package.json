{
  "name": "todo-app",
  "version": "0.0.0",
  "scripts": {
    "setup": "npm run symlink && npm run glob",
    "glob": "glob-module-file --pattern='client/route/**/*.route.js' --format=es --pathPrefix='../' --outputPath=client/globbed-routes.js",
    "prebuild": "npm run setup",
    "build": "rollup -c",
    "dev": "npm run setup && npm-run-all --parallel watch:js watch:glob",
    "start": "sirv public --dev --single --port 3000",
    "watch:js": "rollup -c -w",
    "watch:glob": "watch 'npm run glob' client/route",
    "symlink": "rimraf client/node_modules && mkdirp client/node_modules && cd client/node_modules && symlink ../component/ component && symlink ../lib/ lib"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "glob-module-file": "^3.0.2",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.11",
    "svelte": "^3.0.0",
    "symlink-cli": "^1.0.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "abstract-state-router": "^6.1.0",
    "svelte-state-renderer": "^3.0.1"
  }
}
